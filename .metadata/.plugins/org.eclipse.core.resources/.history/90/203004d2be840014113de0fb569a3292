package logic;

import data_structure.Heap;
import dijsktra.Map;

public class Car extends Thread {

	private Order[] currentOrders;
	private int currentCity;
	private boolean isFull;
	private Map map;
	private Buffer buf;
	private Heap<Order> priorityQueue;
	private Order currentOrder;

	public Car(Parser p, Buffer b) {
		map = p.getMap();
		buf = b;
		priorityQueue = p.getHeap();
	}

	public void run() {
		currentOrder = priorityQueue.pop();
		int value = map.getShortestPath(currentOrder.getBaseCity(),currentOrder.getOrderCity());
	}

}
