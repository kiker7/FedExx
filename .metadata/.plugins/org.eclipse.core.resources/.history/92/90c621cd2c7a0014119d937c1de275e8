package logic;

public class Order implements Comparable<Order> {

	private int id;
	private int baseCity;
	private int orderCity;
	private String name;
	private int priority;
	
	public Order(int iD, int bCity, int oCity, String nm, int pt){
		id = iD;
		baseCity = bCity;
		orderCity = oCity;
		name = nm;
		priority = pt;
	}

	@Override
	public int compareTo(Order o) {	
		return priority - o.priority;
	}
	
}


